[Unit]
Description=Run FRPS Script at Startup
Wants=network-online.target
After=network-online.target

[Service]
Type=simple
ExecReload=/bin/kill -HUP $MAINPID
#需要根据目录位置进行修改
ExecStart=/usr/local/bin/frps --config /home/nas/frp/server/frps.toml
Restart=always
RestartSec=3

[Install]
WantedBy=default.target